decl
	integer factorial(integer n; integer x; integer y);
	integer f();
enddecl

integer factorial (integer n) {
	decl
		integer rvalue;
		integer n;
	enddecl
begin
	if (n==1) then
		rvalue = 1;
	else
		rvalue = n;
	endif;
return 0; 
end
}

integer f()
{
	decl
		integer a;
	enddecl
begin
	a = 10;
end
}

integer main( ){
decl
	integer x,i ;
enddecl
begin
	read (n);
	i = 1;
	while ( i <= n) do
		write ( 1);
		i = i + 1;
	endwhile;
	return 1;
end
}
